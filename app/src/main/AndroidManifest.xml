<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookinghotelonline">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DetailMeetingRoomActivity"></activity>
        <activity android:name=".DetailHotelRoomActivity" />
        <activity android:name=".MeetingRoomActivity" />
        <activity android:name=".HotelRoomActivity" />
        <activity
            android:name=".SignupActivity"
            android:label="@string/title_activity_signup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ProfilActivity" />
        <activity android:name=".HomeActivity">
        </activity>
        <activity android:name=".BookingMenuActivity">
        </activity>
        <activity android:name=".bookingFormHotelRoomActivity" />
        <activity android:name=".bookingFormMeetingRoomActivity" />
        <activity android:name=".editProfileActivity" />
        <activity android:name=".FacilitiesActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>